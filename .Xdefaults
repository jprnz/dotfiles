!! Colorscheme: Jellybeans
*foreground:    #ffffff
*background:    #000000
*cursorColor:   #d0d0d0
 
!black
*color0:#262626
*color8:#606060

!red
*color1: #ff5f00
*color9:#ff8700

!yellow
*color3:#ffffaf
*color11:#ffd75f

!blue
*color4:#87afdf
*color12:#a6a6de

!green
*color2:#d7ffaf
*color10:#d7ff5f

!magenta
*color5:#af87df
*color13:#dfafdf

!cyan
*color6:#ffff87
*color14:#ffaf5f

!white
*color7:#ffffff
*color15:#e9e9e7


! Xft settings
Xft.antialias:  true
Xft.autohitnt:  true
Xft.hinting:    true
Xft.dpi:        96
Xft.hintstyle:  hintsslight
Xft.lcdfilter:  lcdnone
 
URxvt*font:           -misc-tamzenforpowerline-medium-r-normal--14-101-100-100-c-70-iso8859-1 
URxvt*boldFont:       -misc-tamzenforpowerline-bold-r-normal--14-101-100-100-c-70-iso8859-1 
URxvt*italicFont:     -misc-tamzenforpowerline-medium-r-normal--14-101-100-100-c-70-iso8859-1 
URxvt*bolditalicFont: -misc-tamzenforpowerline-bold-r-normal--14-101-100-100-c-70-iso8859-1 

URxvt*geometry: 80x73
URxvt*letterSpace: 0
URxvt*depth: 32
URxvt*borderLess: false
URxvt*scrollBar: false
URxvt*saveLines: 5000

!! Copy Paste & Other Extensions
URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,selection-to-clipboard
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.keysym.M-y: perl:clipboard:copy
URxvt.keysym.M-p: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.urlLauncher: firefox
URxvt.underlineURLs: true
URxvt.urlButton: 1

emacs.font: -misc-tamzenforpowerline-medium-r-normal--14-101-100-100-c-70-iso8859-1 
 
